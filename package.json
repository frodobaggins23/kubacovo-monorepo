{
  "name": "@kubacovo-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "container:start": "sudo docker compose up -d ssh-dev",
    "container:stop": "sudo docker compose stop ssh-dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "files:staged": "git diff --cached --name-only --diff-filter=ACMR | grep -E '\\.(js|jsx|ts|tsx|json|md|css|scss)$'",
    "format:staged": "npm run files:staged --silent| xargs -r prettier --write --list-different",
    "ts:staged": "NX_TUI=false npx nx affected --target=typecheck --base=HEAD --verbose --nx-bail=false",
    "postinstall": "node scripts/install-git-hooks.js",
    "design-system:ask": "claude -r 01e9adc0-7c0b-4db0-a078-edb8ca35ce35 -p"
  },
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
